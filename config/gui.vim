colorscheme darcula
" Default charset
"set anti enc=utf-8
" Remove menubar
set guioptions-=m
" Remove toolbar
set guioptions-=T
" Remove right scrollbar
set guioptions-=r
" Remove left scrollbar
set guioptions-=L
" Remove graphical tab panel
set guioptions-=e
" Console dialogs instead of graphical
set guioptions+=c
" Fullscreen mode
set fu
" Cursorline
set cursorline
" Font
set guifont=Source\ Code\ Pro\ 11
" Tabs panel
set showtabline
